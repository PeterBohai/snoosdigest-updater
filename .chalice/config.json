{
    "version": "1.0",
    "app_name": "snoosdigest-updater",
    "environment_variables": {
        "MAX_NUM_POSTS_PER_SUBREDDIT": "5"
    },
    "stages": {
        "dev": {
            "environment_variables": {
                "IS_PROD_SYSTEM": "false"
            }
        },
        "prod": {
            "environment_variables": {
                "IS_PROD_SYSTEM": "true"
            }
        }
    },
    "lambda_functions": {
        "reddit-posts": {
            "lambda_timeout" : 270
        },
        "subreddit-list": {
            "lambda_timeout" : 900
        }
    }
}
